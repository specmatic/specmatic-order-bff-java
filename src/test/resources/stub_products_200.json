{
  "data": {
    "@productType": {
      "gadget": {
        "name": "iPhone"
      }
    }
  },
  "http-request": {
    "method": "GET",
    "path": "/products",
    "query": {
      "type": "{{@productType}}"
    }
  },
  "http-response": {
    "status": 200,
    "body": [
      {
        "id": 1,
        "name": "{{@productType}}",
        "inventory": 10,
        "type": "{{REQUEST.QUERY-PARAMS.type}}"
      }
    ],
    "status-text": "OK"
  }
}